// задача 5.
let monthNumber = prompt ('какой сегодня по счету месяц');
switch (monthNumber) {
    case '1':
        alert('зима')
        break;
    case '2':
        alert('зима')
        break;
    case '3':
        alert('зима')
    break;
    case '4':
        alert('весна')
        break;
    case '5':
        alert('весна')
        break;
    case '6':
        alert('весна')
        break;
    case '7':
        alert('лето')
        break;
    case '8':
        alert('лето')
        break;
    case '9':
        alert('лето')
    break;
    case '10':
        alert('осень')
        break;
    case '11':
        alert('осень')
        break;
    case '12':
        alert('осень')
        break;
    default:
        alert('ошибаешься')
        break;
};